{
    "name": "netflix",
    "description": "netflix rearanger",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },

    "browser_action": {

    },
    "permissions": [
        "tabs",
        "*://*.netflix.com/*",
        "storage"
    ],
    "content_scripts": 
    [
        {
            "matches":["*://*.netflix.com/*"],
            "js":["foreground.js"],
            "css": ["styles.css"]
        }
    ]
}